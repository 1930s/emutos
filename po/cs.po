msgstr "# v prvním neprázdném sloupci je komentář."

#: cli/command.c:754
msgid ""
"CAT or TYPE filenm.ext\r\n"
"\tWrites filenm.ext to standard output."
msgstr ""
"CAT nebo TYPE soubor.ext\r\n"
"\tVypíše soubor.ext na standardní výstup."

#: cli/command.c:757
msgid ""
"CD [pathnm]\r\n"
"\tWith pathnm it sets default for working directory.\r\n"
"\tWithout pathnm displays current working directory."
msgstr ""
"CD [cesta]\r\n"
"\tJe-li zadána cesta, nastaví ji jako pracovní adresář.\r\n"
"\tBez zadané cesty vypíše aktuální pracovní adresář."

#: cli/command.c:761
msgid ""
"CHMOD [pathnm/]filenm mode\r\n"
"\tChanges the mode of the file specified in filenm to the\r\n"
"\tvalue of mode.\tAcceptable values are < 7:\r\n"
"\t\t0 - Normal File Entry\r\n"
"\t\t1 - File is Read Only\r\n"
"\t\t2 - File is Hidden from directory Search\r\n"
"\t\t4 - File is System File"
msgstr ""
"CHMOD [cesta/]soubor režim\r\n"
"\tZmění režim souboru zadaného v položce soubor na\r\n"
"\thodnotu uvedenou jako režim.\tPřípustné hodnoty jsou < 7:\r\n"
"\t\t0 - Normální soubor\r\n"
"\t\t1 - Soubor jen pro čtení\r\n"
"\t\t2 - Soubor je skrytý pro hledání v adresáři\r\n"
"\t\t4 - Soubor je systémovým souborem"

#: cli/command.c:770
msgid "\tClears the screen."
msgstr "\tSmaže obrazovku."

#: cli/command.c:771
msgid ""
"COPY source_file [destination_file]\r\n"
"\tCopies source to destination."
msgstr ""
"COPY zdrojový_soubor [cílový_soubor]\r\n"
"\tZkopíruje zdroj do cíle."

#: cli/command.c:774
msgid ""
"DIR or LS [filenm.ext] [-f] [-d] [-t] [-w]\r\n"
"\t-f - anything but directoryies.\r\n"
"\t-d - directories only.\r\n"
"\t-t - terse: names only.\r\n"
"\t-w - wide: names only displayed horizontally."
msgstr ""
"DIR nebo LS [soubor.ext] [-f] [-d] [-t] [-w]\r\n"
"\t-f - všechno kromě adresářů.\r\n"
"\t-d - pouze adresáře.\r\n"
"\t-t - stručný: pouze jména souborů.\r\n"
"\t-w - široký: pouze jména zobrazena po řádcích."

#: cli/command.c:783
msgid "\tDisplays the value of the Completion Code for the last command."
msgstr "\tZobrazí hodnotu návratového kódu posledního příkazu."

#: cli/command.c:787
msgid "\tExits CLI to invoking program."
msgstr "\tOpustí CLI a spustí program."

#: cli/command.c:789
msgid ""
"INIT [drive_spec:]\r\n"
"\tReinitializes FAT entries this wiping disk."
msgstr ""
"INIT [zařízení:]\r\n"
"\tReinicializuje FAT záznamy daného smazaného disku."

#: cli/command.c:792
msgid ""
"MD [subdirectory name]\r\n"
"\tCreates a new subdirectory in current directory."
msgstr ""
"MD [jméno podadresáře]\r\n"
"\tVytvoří nový podadresář v aktuálním adresáři."

#: cli/command.c:795
msgid ""
"MOVE source_file [destination_file]\r\n"
"\tCopies source to destination and deletes source."
msgstr ""
"MOVE zdrojový_soubor [cílový_soubor]\r\n"
"\tZkopíruje zdroj do cíle a smaže zdroj."

#: cli/command.c:799
msgid ""
"\tWrites 'CR to continue...' to standard output\r\n"
"\tand waits for a carriage return from standard input."
msgstr ""
"\tVypíše 'CR pro pokračování...' na standardní výstup\r\n"
"\ta čeká na návrat vozíku ze standardního vstupu."

#: cli/command.c:803
msgid ""
"\tTurns command processing abort feature ON/OFF.\r\n"
"\tIf PRGERR is ON and a .PRG file returns a non zero completion\r\n"
"\tcode, all further processing will stop.  Usefull in .BAT files.\r\n"
"\tDefault is ON."
msgstr ""
"\tZmění příznak zrušení zpracování příkazu na ON/OFF.\r\n"
"\tPokud je PRGERR na ON a soubor .PRG vrátí nenulový návratový\r\n"
"\tkód, veškeré další zpracování bude zastaveno.  Užitečné v .BAT souborech.\r\n"
"\tImplicitní je ON."

#: cli/command.c:809
msgid "\tDisables line wrap."
msgstr "\tZablokuje zalomení řádky."

#: cli/command.c:810
msgid ""
"PATH [;[pathnm]...]\r\n"
"\tWith path name sets default path for batch and commands.\r\n"
"\tWithout path name displays current path"
msgstr ""
"PATH [;[cesta]...]\r\n"
"\tPři udání cesty nastaví implicitní cestu pro dávky a příkazy.\r\n"
"\tBez udání cesty zobrazí aktuální adresář"

#: cli/command.c:816
msgid ""
"REM or ECHO [\"string\"]\r\n"
"\tStrips quotes and writes string to standard output.\r\n"
"\tr is replaced by 13, /n by 10, /0 by 0x0.\r\n"
"\t/c by 13 10, /anything is replaced by anything."
msgstr ""
"REM nebo ECHO [\"řetězec\"]\r\n"
"\tOdstraní uvozovky a vypíše řetězec na standardní výstup.\r\n"
"\tr bude nahrazeno pomocí 13, /n pomocí 10, /0 pomocí 0x0.\r\n"
"\t/c pomocí 13 10, /cokoli bude nahrazeno řetězcem cokoli."

#: cli/command.c:821
msgid ""
"REN source_file_nm [destination_file_nm]\r\n"
"\tRenames source to destination."
msgstr ""
"REN zdrojový_soubor [cílový_soubor]\r\n"
"\tPřejmenuje zdroj na cíl."

#: cli/command.c:824
msgid ""
"RD [pathnm]\r\n"
"\tRemoves named directory."
msgstr ""
"RD [cesta]\r\n"
"\tOdstraní uvedený adresář."

#: cli/command.c:827
msgid ""
"RM or DEL or ERA filenm [[filemn]...] [-q]\r\n"
"\tRemoves named file from directory.\r\n"
"\tIf the -q option is used, the CLI will display the question\r\n"
"\tY/CR... and wait for a response."
msgstr ""
"RM nebo DEL nebo ERA soubor [[soubor]...] [-q]\r\n"
"\tOdstraní uvedený soubor z adresáře.\r\n"
"\tJe-li použita volba -q, CLI zobrazí otázku\r\n"
"\tY/CR... a vyčká na odpověď."

#: cli/command.c:832
msgid ""
"SHOW [drive_spec:]\r\n"
"\tDisplays disk status for default drive or drive specified."
msgstr ""
"SHOW [zařízení:]\r\n"
"\tZobrazí stav implicitního nebo zadaného disku."

#: cli/command.c:836
