#: cli/command.c:653
msgid ""
"CAT or TYPE filenm.ext\015\n"
"\tWrites filenm.ext to standard output."
msgstr ""
"CAT ou TYPE fichier.ext\r\n"
"\tEcrit fichier.ext sur la sortie standard."

#: cli/command.c:656
msgid ""
"CD [pathnm]\015\n"
"\tWith pathnm it sets default for working directory.\015\n"
"\tWithout pathnm displays current working directory."
msgstr ""
"CD [chemin]\r\n"
"\tSi le chemin est spécifié, définit le répertoire courant.\r\n"
"\tSinon, affiche le répertoire courant."

#: cli/command.c:660
msgid ""
"CHMOD [pathnm/]filenm mode\015\n"
"\tChanges the mode of the file specified in filenm to the\015\n"
"\tvalue of mode.\tAcceptable values are < 7:\015\n"
"\t\t0 - Normal File Entry\015\n"
"\t\t1 - File is Read Only\015\n"
"\t\t2 - File is Hidden from directory Search\015\n"
"\t\t4 - File is System File"
msgstr ""
"CHMOD [chemin]/fichier mode\r\n"
"\tChange le mode du fichier. Le mode est un entier < 7:\r\n"
"\t\t0 - Fichier normal\r\n"
"\t\t1 - Fichier en lecture seule\r\n"
"\t\t2 - Fichier caché\r\n"
"\t\t4 - fichier système"

#: cli/command.c:670
msgid ""
"COPY source_file [destination_file]\015\n"
"\tCopies source to destination."
msgstr ""
"COPY fichier_source [fichier_destination]\r\n"
"\tCopie la source sur la destination."

#: cli/command.c:673
msgid ""
"DIR or LS [filenm.ext] [-f] [-d] [-t] [-w]\015\n"
"\t-f - anything but directoryies.\015\n"
"\t-d - directories only.\015\n"
"\t-t - terse: names only.\015\n"
"\t-w - wide: names only displayed horizontally."
msgstr ""
"DIR ou LS [fichier.ext] [-f] [-d] [-t] [-w]\r\n"
"\t-f - n'affiche pas les répertoires.\r\n"
"\t-d - affiche uniquement les répertoires.\r\n"
"\t-t - (terse) affiche uniquement les noms.\r\n"
"\t-w - (wide) uniquement les noms, horizontalement."

#: cli/command.c:686
msgid ""
"INIT [drive_spec:]\015\n"
"\tReinitializes FAT entries this wiping disk."
msgstr ""
"INIT [lecteur:]\r\n"
"\tRéinitialise les entrées de la FAT, ce qui efface le disque."

#: cli/command.c:689
msgid ""
"MD [subdirectory name]\015\n"
"\tCreates a new subdirectory in current directory."
msgstr ""
"MD [sous_répertoire]\r\n"
"\tCrée un nouveau sous-répertoire dans le répertoire courant."

#: cli/command.c:692
msgid ""
"MOVE source_file [destination_file]\015\n"
"\tCopies source to destination and deletes source."
msgstr ""
"MOVE fichier_source [fichier_destination]\r\n"
"\tCopie la source vers la destination, puis efface la source."

#: cli/command.c:696
msgid ""
"\tWrites 'CR to continue...' to standard output\015\n"
"\tand waits for a carriage return from standard input."
msgstr ""
"\tEcrit 'CR pour continuer...' sur la sortie standard\r\n"
"\tet attend un retour chariot sur l'entrée standard."

#: cli/command.c:700
msgid ""
"\tTurns command processing abort feature ON/OFF.\015\n"
"\tIf PRGERR is ON and a .PRG file returns a non zero completion\015\n"
"\tcode, all further processing will stop.  Usefull in .BAT files.\015\n"
"\tDefault is ON."
msgstr ""
"\tactive/désactive l'abandon de traitement de commande.\r\n"
"\tSi PRGERR est sur ON et si un fichier .PRG retourne une valeur\r\n"
"\tnon nulle, tout traitement ultérieur est annullé. Utile dans les\r\n"
"\tfichiers .BAT. Par défaut sur ON."

#: cli/command.c:707
msgid ""
"PATH [;[pathnm]...]\015\n"
"\tWith path name sets default path for batch and commands.\015\n"
"\tWithout path name displays current path"
msgstr ""
"PATH [;[chemin]...]\r\n"
"\tSi le chemin est spécifié, définit le chemin par défaut pour\r\n"
"\tles commandes et les .BAT; sinon affiche le chemin en cours"

#: cli/command.c:712
msgid ""
"REM or ECHO [\"string\"]\015\n"
"\tStrips quotes and writes string to standard output.\015\n"
"\tr is replaced by 13, /n by 10, /0 by 0x0.\015\n"
"\t/c by 13 10, /anything is replaced by anything."
msgstr ""
"REM ou ECHO [\"chaîne\"]\r\n"
"\tRetire les guillemets et affiche la chaîne sur la sotrtie standard.\r\n"
"\t/r est remplacé par 13, /n par 10, /0 par 0, /c par 13 10,\r\n"
"\t/ suivi d'un autre caractère est remplacé par ce caractère." 

#: cli/command.c:717
msgid ""
"REN source_file_nm [destination_file_nm]\015\n"
"\tRenames source to destination."
msgstr ""
"REN fichier_source [fichier_destination]\r\n"
"\trenomme le fichier source en destination."

#: cli/command.c:720
msgid ""
"RD [pathnm]\015\n"
"\tRemoves named directory."
msgstr ""
"RD [chemin]\r\n"
"\tRetire le répertoire spécifié."

#: cli/command.c:723
msgid ""
"RM or DEL or ERA filenm [[filemn]...] [-q]\015\n"
"\tRemoves named file from directory.\015\n"
"\tIf the -q option is used, the CLI will display the question\015\n"
"\tY/CR... and wait for a response."
msgstr ""
"RM ou DEL ou ERA fichier [[fichier]...] [-q]\r\n"
"\tRetire du répertoire le fichier spécifié.\r\n"
"\tSi l'option -q est utilisée, la question «Y/CR...» sera posée\r\n"
"\tet EmuCON attendra la réponse."

#: cli/command.c:728
msgid ""
"SHOW [drive_spec:]\015\n"
"\tDisplays disk status for default drive or drive specified."
msgstr ""
"SHOW [lecteur:]\r\n"
"\tAffiche l'état du lecteur par défaut, ou du lecteur spécifié."


